<h1>Listing projects</h1>
 
 <p>
 
   <%= form_tag(projects_path, :method => "get", class: "navbar-form", id: "search-form") do %>
    <div class="input-append">
      <strong>Search:</strong> 
      <%= text_field_tag :search, params[:search], class: "span2", placeholder: "Search Projects" %>
      <%= select_tag :options, options_for_select(["title", "company", "start date", "end date"], params[:options]) %>

      <!-- In order to have the "search" icon int the button, we need to use plain HTML instead 
           of using a Rails form helper -->
      <button class="btn" type="submit"><i class="icon-search"></i></button>
    </div>
    <% end %>
</p>
 
<table>
  <tr>
    <th>Title</th>
    <th>Company</th>
	<th>Start date</th>
	<th>End date</th>
  <th>Description</th>
  <th>Team Members</th>
  </tr>
 
  <% @projects.each do |project| %>
    <tr>
		<td><%= project.title %></td>
		<td><%= project.company %></td>
		<td><%= project.start_date %></td>
		<td><%= project.end_date %></td>
		<td><%= project.description %></td>
		<td><%= link_to 'Show', project_path(project) %></td>
		<td><%= link_to 'Edit', edit_project_path(project) %></td>
		<td><%= link_to 'Destroy', project_path(project),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
	  
    </tr>
  <% end %>
</table>

<%= link_to 'Front Page', '/' %>

